{
  "name": "version-ts-in-babel",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "rm-dist": "rm -Rf ./dist",
    "build-dev": "yarn run rm-dist && webpack --config ./webpack.js --mode development && cp ./src/index.html ./dist/",
    "build-prod": "yarn run rm-dist && webpack --config ./webpack.js --mode production  && cp ./src/index.html ./dist/",
    "watch-dev": "yarn run rm-dist && mkdir ./dist && cp ./src/index.html ./dist/ && webpack --config ./webpack.js --mode development --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check -- --watch",
    "server": "http-server ./dist"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/runtime": "^7.2.0",
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@rebass/grid": "^6.0.0-7",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-modal": "^3.8.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.6",
    "color": "^3.1.0",
    "http-server": "^0.11.1",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-modal": "^3.8.1",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
